{
    "Version": "2012-10-17",
    "Statement": [
                      {
            "Sid": "AllowListAllBuckets",
            "Action": "s3:ListAllMyBuckets",
            "Effect": "Allow",
            "Resource": "arn:aws:s3:::*"
        },
          {
              "Sid": "AllObjectActions",
              "Action": [
                  "s3:Get*",
                  "s3:List*"
              ],
              "Effect": "Allow",
              "Resource": [
                  "arn:aws:s3:::{{bucketName}}",
                  "arn:aws:s3:::{{bucketName}}/*"
              ]
          }
    ]
}